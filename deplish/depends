import sys
from PySide import QtGui, QtCore
import depends_main_window


if __name__ == '__main__':
    app = QtGui.QApplication(sys.argv)

    with open('darkorange.css', 'r') as f:
        app.setStyleSheet(f.read())

    win = depends_main_window.MainWindow()
    win.show()

    sys.exit(app.exec_())
